        - waitTaskRepositoriesByKeyWord(isRunTaskNow githubApiService.IsRunTaskNow, taskLaunchFunction githubApiService.TaskLaunchFunction, channelNotificationRateLimit <font color=blue>chan</font> bool, channelGettingTaskState <font color=blue>chan</font> *githubApiService.TaskState) 

        + ConcatTheirRestHandlers(engine *gin.Engine) 
        + CreateTaskRepositoriesDescriptions(jsonModel *JsonCreateTaskRepositoriesDescriptions) error
        + CreateTaskRepositoryIssues(jsonModel *JsonCreateTaskRepositoryIssues) error
        + CreateTaskRepositoriesByKeyWord(jsonModel *JsonCreateTaskRepositoriesByKeyWord) error

